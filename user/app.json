{
  "expo": {
    "name": "Tvornica snage",
    "slug": "tvornica-snage",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Tvornica snage accesses your photos to use photo as profile image",
          "cameraPermission":  "Tvornica snage accesses your camera to take photo for profile image"
        }
      ],
      "expo-community-flipper"

    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "tvornicaSnageUser",
      "buildNumber": "6"
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tvornica_snage.tvornicasnage"
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "6b96f4ad-a02c-47de-ad50-8ed3a3b0b705"
      }
    },
    "owner": "podra01"
  }
}
